<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.dao.TestInfoDao">
    <insert id = "insertTestInfo" parameterType = "com.study.vo.TestInfoVo">
        insert into testInfo(SNAME, SPHONE, sDepartment, sNum)
        values(#{sName}, #{sPhone}, #{sDepartment}, sName_seq.nextval)
    </insert>

    <select id = "searchTestInfo" resultType="com.study.vo.TestInfoVo">
        SELECT * FROM TESTINFO
    </select>

</mapper>
